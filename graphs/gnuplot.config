set terminal postscript eps color lw 1 "Helvetica" 14
set output "(database).(classifier).eps"
set key over nobox
set style data linespoints
set style line 1 lt 10 lc rgb "violet" lw 1
set style line 2 lt 5 lc rgb "orange" lw 1
plot    '(database).(classifier).dat' u 2:xtic(1) t "iteracoes", \
        '' u 3 t "pares", \
        (cls_mean) ls 1 t "(classifier)", \
        (ranking_mean) ls 2 t "ranking"
